{
  "name": "Tab Manipulator",
  "description": "Sort tabs, merge windows, extract domain and other tab manipulation.",
  "version": "1.0",
  "author": "Lau Kondrup",
  "homepage_url": "https://github.com/LK28/tabSorter",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
    // "bookmarks"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}